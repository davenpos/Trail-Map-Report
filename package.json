{
  "name": "trailmapreport",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "npm-run-all build:*",
    "build:wp": "wp-scripts build",
    "build:imagemap": "npm run imagemap",
    "build:form": "npm run form",
    "dev": "concurrently \"wp-scripts start\" \"npm run extra-scss\"",
    "extra-scss": "wait-on build/index.js && npm-run-all --parallel scss:*",
    "scss:imagemap": "npm run imagemap -- --watch",
    "scss:form": "npm run form -- --watch",
    "imagemap": "sass css/_imagemap.scss:build/imagemap.css --no-source-map",
    "form": "sass css/_form.scss:build/form.css --no-source-map",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@wordpress/scripts": "^30.19.0",
    "concurrently": "^9.2.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.89.2",
    "slugify": "^1.6.6",
    "wait-on": "^8.0.3"
  }
}